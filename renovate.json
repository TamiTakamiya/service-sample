{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "packageRules": [
      {
        "matchDatasources": ["docker"],
        "matchFileNames": ["Containerfile"],
        "matchPackageNames": [
          "quay.io/redhat-user-workloads/rh-ee-ttakamiy-tenant/content-sample"
        ],
        "versioning": "semver"
      }
    ],
    "dockerfile": {
      "enabled": true,
      "schedule": ["* * * * *"]
    },
    "automerge": true
  }